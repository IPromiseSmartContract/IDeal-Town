{
  "name": "ideal-town",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "postinstall": "yarn generate",
    "generate": "typechain --target ethers-v5 --out-dir ./src/contracts --glob \"../artifacts/contracts/**/!(*dbg).json\" "
  },
  "dependencies": {
    "@blocto/sdk": "^0.4.1",
    "@metamask/providers": "^10.2.1",
    "@types/archiver": "^5.3.2",
    "@unirep/core": "^2.0.0-beta-3",
    "@vavt/md-editor-extension": "^2.0.0",
    "buffer": "^6.0.3",
    "ethers": "5.6.8",
    "ipfs-core": "^0.18.0",
    "jszip": "^3.10.1",
    "md-editor-v3": "^2.11.2",
    "pinia": "^2.0.32",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.26.1",
    "vee-validate": "^4.8.6",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "web3": "^1.9.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typechain": "^8.1.1",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  }
}
